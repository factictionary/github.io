    <!-- JavaScript -->
    <script>
        // Global variables
        let currentVideo = null;
        let extractedFrames = [];

        // Initialize on page load
        document.addEventListener('DOMContentLoaded', function() {
            initializeEventListeners();
        });

        function initializeEventListeners() {
            // File upload handlers
            const uploadArea = document.getElementById('uploadArea');
            const videoInput = document.getElementById('videoInput');
            const videoSection = document.getElementById('videoSection');

            // Drag and drop handlers
            uploadArea.addEventListener('click', () => videoInput.click());
            uploadArea.addEventListener('dragover', handleDragOver);
            uploadArea.addEventListener('dragleave', handleDragLeave);
            uploadArea.addEventListener('drop', handleDrop);
            videoInput.addEventListener('change', handleFileSelect);

            // Video control handlers
            const videoPlayer = document.getElementById('videoPlayer');
            const timelineSlider = document.getElementById('timelineSlider');

            videoPlayer.addEventListener('loadedmetadata', updateVideoInfo);
            videoPlayer.addEventListener('timeupdate', updateTimeline);
            timelineSlider.addEventListener('input', seekVideo);

            // Extraction mode handlers
            const extractionModes = document.querySelectorAll('input[name="extractionMode"]');
            extractionModes.forEach(mode => {
                mode.addEventListener('change', toggleExtractionOptions);
            });
        }

        function handleDragOver(e) {
            e.preventDefault();
            document.getElementById('uploadArea').classList.add('drag-over');
        }

        function handleDragLeave(e) {
            e.preventDefault();
            document.getElementById('uploadArea').classList.remove('drag-over');
        }

        function handleDrop(e) {
            e.preventDefault();
            document.getElementById('uploadArea').classList.remove('drag-over');
            const files = e.dataTransfer.files;
            if (files.length > 0) {
                processVideoFile(files[0]);
            }
        }

        function handleFileSelect(e) {
            const file = e.target.files[0];
            if (file) {
                processVideoFile(file);
            }
        }

        function processVideoFile(file) {
            // Validate file type
            if (!file.type.startsWith('video/')) {
                showNotification('‚ùå Please select a valid video file');
                return;
            }

            // Validate file size (500MB limit)
            if (file.size > 500 * 1024 * 1024) {
                showNotification('‚ùå File size too large. Maximum 500MB allowed.');
                return;
            }

            // Create video URL
            const videoURL = URL.createObjectURL(file);
            const videoPlayer = document.getElementById('videoPlayer');
            videoPlayer.src = videoURL;
            
            // Show video section
            document.getElementById('videoSection').style.display = 'block';
            
            showNotification('üìπ Video loaded successfully!');
        }

        function updateVideoInfo() {
            const video = document.getElementById('videoPlayer');
            const duration = formatTime(video.duration);
            
            document.getElementById('totalTime').textContent = duration;
            document.getElementById('scrubberTime').textContent = '00:00';
        }

        function updateTimeline() {
            const video = document.getElementById('videoPlayer');
            const current = formatTime(video.currentTime);
            const total = formatTime(video.duration);
            const progress = (video.currentTime / video.duration) * 100;
            
            document.getElementById('currentTime').textContent = current;
            document.getElementById('timelineSlider').value = progress;
            document.getElementById('scrubberTime').textContent = current;
        }

        function seekVideo() {
            const video = document.getElementById('videoPlayer');
            const slider = document.getElementById('timelineSlider');
            const seekTime = (slider.value / 100) * video.duration;
            
            video.currentTime = seekTime;
        }

        function toggleExtractionOptions() {
            const mode = document.querySelector('input[name="extractionMode"]:checked').value;
            const rangeSettings = document.getElementById('rangeSettings');
            const intervalSettings = document.getElementById('intervalSettings');
            
            // Hide all option sections
            rangeSettings.style.display = 'none';
            intervalSettings.style.display = 'none';
            
            // Show relevant options
            if (mode === 'range') {
                rangeSettings.style.display = 'block';
            } else if (mode === 'every') {
                intervalSettings.style.display = 'block';
            }
        }

        function formatTime(seconds) {
            const mins = Math.floor(seconds / 60);
            const secs = Math.floor(seconds % 60);
            return `${mins.toString().padStart(2, '0')}:${secs.toString().padStart(2, '0')}`;
        }

        function parseTime(timeStr) {
            const parts = timeStr.split(':');
            if (parts.length === 2) {
                return parseInt(parts[0]) * 60 + parseInt(parts[1]);
            }
            return 0;
        }

        async function extractFrames() {
            const video = document.getElementById('videoPlayer');
            const mode = document.querySelector('input[name="extractionMode"]:checked').value;
            const format = document.getElementById('outputFormat').value;
            
            if (!video.src) {
                showNotification('‚ùå Please load a video first');
                return;
            }

            // Disable extract button during processing
            const extractButton = document.getElementById('extractButton');
            extractButton.disabled = true;
            extractButton.textContent = '‚è≥ Extracting...';
            
            try {
                let frameTimes = [];
                
                if (mode === 'single') {
                    frameTimes = [video.currentTime];
                } else if (mode === 'range') {
                    const startTime = parseTime(document.getElementById('startTime').value);
                    const endTime = parseTime(document.getElementById('endTime').value);
                    
                    if (startTime >= endTime) {
                        throw new Error('End time must be greater than start time');
                    }
                    
                    // Extract frames every second within range
                    for (let t = startTime; t <= endTime; t++) {
                        frameTimes.push(t);
                    }
                } else if (mode === 'every') {
                    const intervalValue = parseFloat(document.getElementById('intervalValue').value);
                    const intervalUnit = document.getElementById('intervalUnit').value;
                    
                    let interval = intervalValue;
                    if (intervalUnit === 'minutes') {
                        interval *= 60;
                    }
                    
                    for (let t = 0; t <= video.duration; t += interval) {
                        frameTimes.push(t);
                    }
                }
                
                extractedFrames = [];
                
                // Show progress
                showNotification(`üé¨ Extracting ${frameTimes.length} frames...`);
                
                for (let i = 0; i < frameTimes.length; i++) {
                    const time = frameTimes[i];
                    video.currentTime = time;
                    
                    // Wait for video to seek
                    await new Promise(resolve => {
                        video.addEventListener('seeked', resolve, { once: true });
                    });
                    
                    // Extract frame using Canvas
                    const canvas = document.createElement('canvas');
                    canvas.width = video.videoWidth;
                    canvas.height = video.videoHeight;
                    
                    const ctx = canvas.getContext('2d');
                    ctx.drawImage(video, 0, 0, canvas.width, canvas.height);
                    
                    // Convert to blob
                    const blob = await new Promise(resolve => {
                        canvas.toBlob(resolve, `image/${format}`, format === 'jpg' ? 0.9 : 1);
                    });
                    
                    const frameData = {
                        blob: blob,
                        url: URL.createObjectURL(blob),
                        time: time,
                        filename: `frame_${formatTime(time).replace(':', '-')}.${format}`
                    };
                    
                    extractedFrames.push(frameData);
                    
                    // Update progress
                    if (i % 5 === 0) {
                        showNotification(`üé¨ Extracted ${i + 1}/${frameTimes.length} frames...`);
                    }
                }
                
                displayExtractedFrames();
                showNotification(`‚úÖ Successfully extracted ${extractedFrames.length} frames!`);
                
            } catch (error) {
                console.error('Extraction error:', error);
                showNotification('‚ùå Error extracting frames: ' + error.message);
            } finally {
                // Re-enable extract button
                extractButton.disabled = false;
                extractButton.textContent = 'üé¨ Extract Frames';
            }
        }

        function displayExtractedFrames() {
            const resultsSection = document.getElementById('resultsSection');
            const resultsSummary = document.getElementById('resultsSummary');
            const framesGrid = document.getElementById('framesGrid');
            const downloadAllButton = document.getElementById('downloadAllButton');
            
            // Show results section
            resultsSection.style.display = 'block';
            
            // Update summary
            resultsSummary.innerHTML = `
                <div class="summary-stats">
                    <div class="stat-item">
                        <span class="stat-value">${extractedFrames.length}</span>
                        <span class="stat-label">Frames Extracted</span>
                    </div>
                    <div class="stat-item">
                        <span class="stat-value">${formatTime(extractedFrames[extractedFrames.length - 1].time)}</span>
                        <span class="stat-label">Max Time</span>
                    </div>
                    <div class="stat-item">
                        <span class="stat-value">${extractedFrames.length > 0 ? Math.round(extractedFrames[0].blob.size / 1024) : 0}KB</span>
                        <span class="stat-label">Avg Size</span>
                    </div>
                </div>
            `;
            
            // Display frames in grid
            framesGrid.innerHTML = '';
            extractedFrames.forEach((frame, index) => {
                const frameCard = document.createElement('div');
                frameCard.className = 'frame-card';
                frameCard.innerHTML = `
                    <div class="frame-image-container">
                        <img src="${frame.url}" alt="Frame ${index + 1}" class="frame-image">
                        <div class="frame-overlay">
                            <span class="frame-time">${formatTime(frame.time)}</span>
                        </div>
                    </div>
                    <div class="frame-info">
                        <span class="frame-filename">${frame.filename}</span>
                        <button class="btn btn-small" onclick="downloadSingleFrame(${index})">
                            üíæ Download
                        </button>
                    </div>
                `;
                framesGrid.appendChild(frameCard);
            });
            
            // Show download all button
            downloadAllButton.style.display = 'inline-block';
        }

        function downloadSingleFrame(index) {
            const frame = extractedFrames[index];
            const link = document.createElement('a');
            link.href = frame.url;
            link.download = frame.filename;
            link.click();
            
            showNotification(`üíæ Downloaded ${frame.filename}`);
        }

        function downloadAllFrames() {
            if (extractedFrames.length === 0) return;
            
            // Create a zip-like behavior by triggering downloads
            extractedFrames.forEach((frame, index) => {
                setTimeout(() => {
                    const link = document.createElement('a');
                    link.href = frame.url;
                    link.download = frame.filename;
                    link.click();
                }, index * 100); // Small delay between downloads
            });
            
            showNotification(`üì¶ Downloading ${extractedFrames.length} frames...`);
        }

        function showNotification(message) {
            // Create and show a temporary notification
            const notification = document.createElement('div');
            notification.className = 'notification';
            notification.textContent = message;
            notification.style.cssText = `
                position: fixed;
                top: 20px;
                right: 20px;
                background: var(--color-success);
                color: white;
                padding: 12px 20px;
                border-radius: var(--radius-button);
                font-weight: 500;
                z-index: 10000;
                animation: slideIn 0.3s ease-out;
            `;
            
            document.body.appendChild(notification);
            
            setTimeout(() => {
                notification.style.animation = 'slideOut 0.3s ease-in';
                setTimeout(() => {
                    document.body.removeChild(notification);
                }, 300);
            }, 3000);
        }

        // Mobile menu functionality
        const menuToggle = document.querySelector('.menu-toggle');
        const navLinks = document.querySelector('.nav-links');
        
        menuToggle.addEventListener('click', () => {
            const isExpanded = menuToggle.getAttribute('aria-expanded') === 'true';
            menuToggle.setAttribute('aria-expanded', !isExpanded);
            navLinks.classList.toggle('active');
        });

        // Add CSS animations for notifications
        const style = document.createElement('style');
        style.textContent = `
            @keyframes slideIn {
                from {
                    transform: translateX(100%);
                    opacity: 0;
                }
                to {
                    transform: translateX(0);
                    opacity: 1;
                }
            }
            
            @keyframes slideOut {
                from {
                    transform: translateX(0);
                    opacity: 1;
                }
                to {
